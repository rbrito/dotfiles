<?xml version="1.0"?>
<anjuta>
    <plugin name="Video File Type Plugin" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="filetype_video:VideoFileTypePlugin"/>
    </plugin>
    <plugin name="Flac File Type Plugin" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="filetype_flac:FlacFileTypePlugin"/>
    </plugin>
    <plugin name="Ogg File Type Plugin" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="filetype_ogg:OGGFileTypePlugin"/>
    </plugin>
    <plugin name="MP4 File Type Plugin" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="filetype_mp4:MP4FileTypePlugin"/>
    </plugin>
    <plugin name="M4A File Type Plugin" mandatory="no">
        <require group="Anjuta Plugin"
                 attribute="Location"
                 value="filetype_m4a:M4AFileTypePlugin"/>
    </plugin>
</anjuta>
